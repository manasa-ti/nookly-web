# Environment Variables Template for Mobile Builds
# Copy this file to .env and fill in your actual values
# NEVER commit .env files to version control!

# ============================================
# 100ms Configuration
# ============================================
HMS_APP_ID=your_hms_app_id_here
HMS_AUTH_TOKEN=your_hms_auth_token_here

# ============================================
# Firebase Configuration (Development)
# ============================================
FIREBASE_ANDROID_DEV_API_KEY=your_firebase_android_dev_api_key
FIREBASE_ANDROID_DEV_APP_ID=your_firebase_android_dev_app_id
FIREBASE_ANDROID_DEV_MESSAGING_SENDER_ID=your_firebase_android_dev_messaging_sender_id
FIREBASE_ANDROID_DEV_PROJECT_ID=your_firebase_android_dev_project_id
FIREBASE_ANDROID_DEV_STORAGE_BUCKET=your_firebase_android_dev_storage_bucket

FIREBASE_IOS_DEV_API_KEY=your_firebase_ios_dev_api_key
FIREBASE_IOS_DEV_APP_ID=your_firebase_ios_dev_app_id
FIREBASE_IOS_DEV_MESSAGING_SENDER_ID=your_firebase_ios_dev_messaging_sender_id
FIREBASE_IOS_DEV_PROJECT_ID=your_firebase_ios_dev_project_id
FIREBASE_IOS_DEV_STORAGE_BUCKET=your_firebase_ios_dev_storage_bucket
FIREBASE_IOS_DEV_BUNDLE_ID=com.nookly.app

# ============================================
# Firebase Configuration (Production)
# ============================================
# Note: Staging also uses production Firebase configs
FIREBASE_ANDROID_PROD_API_KEY=your_firebase_android_prod_api_key
FIREBASE_ANDROID_PROD_APP_ID=your_firebase_android_prod_app_id
FIREBASE_ANDROID_PROD_MESSAGING_SENDER_ID=your_firebase_android_prod_messaging_sender_id
FIREBASE_ANDROID_PROD_PROJECT_ID=your_firebase_android_prod_project_id
FIREBASE_ANDROID_PROD_STORAGE_BUCKET=your_firebase_android_prod_storage_bucket

FIREBASE_IOS_PROD_API_KEY=your_firebase_ios_prod_api_key
FIREBASE_IOS_PROD_APP_ID=your_firebase_ios_prod_app_id
FIREBASE_IOS_PROD_MESSAGING_SENDER_ID=your_firebase_ios_prod_messaging_sender_id
FIREBASE_IOS_PROD_PROJECT_ID=your_firebase_ios_prod_project_id
FIREBASE_IOS_PROD_STORAGE_BUCKET=your_firebase_ios_prod_storage_bucket
FIREBASE_IOS_PROD_BUNDLE_ID=com.nookly.app

# ============================================
# Google Sign-In Configuration
# ============================================
GOOGLE_SIGN_IN_ANDROID_CLIENT_ID=your_google_android_client_id
GOOGLE_SIGN_IN_IOS_CLIENT_ID=your_google_ios_client_id
